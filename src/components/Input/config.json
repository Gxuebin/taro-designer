{
	"type": "Input",
	"name": "输入框",
	"canPlace": false,
	"tips": "该组件是原生组件，使用时请注意相关限制",
	"defaultProps": {
		"type": "text",
		"password": false,
		"placeholder": "请输入内容...",
		"disabled": false,
		"maxLength": 140,
		"focus": false
	},
	"defaultStyles": {},
	"config": [
		{
			"key": "type",
			"type": "Select",
			"label": "input 的类型",
			"dataSource": [
				{
					"label": "number",
					"value": "number"
				},
				{
					"label": "text",
					"value": "text"
				},
				{
					"label": "idcard",
					"value": "idcard"
				},
				{
					"label": "digit",
					"value": "digit"
				}
			]
		},
		{
			"key": "password",
			"label": "是否是密码类型",
			"type": "Radio"
		},
		{
			"key": "placeholder",
			"label": "输入框为空时占位符",
			"type": "Input"
		},
		{
			"key": "disabled",
			"label": "是否禁用",
			"type": "Radio"
		},
		{
			"key": "maxLength",
			"label": "最大输入长度",
			"type": "InputNumber"
		},
		{
			"key": "focus",
			"label": "获取焦点",
			"type": "Radio"
		}
	]
}
