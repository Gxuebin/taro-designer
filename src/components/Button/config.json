{
	"type": "Button",
	"name": "按钮",
	"canPlace": false,
	"defaultProps": {
		"content": "ok",
		"size": "default",
		"type": "default",
		"plain": false,
		"disabled": false,
		"loading": false,
		"hoverClass": "none",
		"hoverStartTime": 20,
		"hoverStayTime": 70
	},
	"defaultStyles": {},
	"config": [
		{
			"key": "content",
			"type": "Input",
			"label": "文案"
		},
		{
			"key": "size",
			"type": "Select",
			"label": "尺寸",
			"dataSource": [
				{
					"label": "default",
					"value": "default"
				},
				{
					"label": "mini",
					"value": "mini"
				}
			]
		},
		{
			"key": "type",
			"type": "Select",
			"label": "主题",
			"dataSource": [
				{
					"label": "default",
					"value": "default"
				},
				{
					"label": "primary",
					"value": "primary"
				},
				{
					"label": "warn",
					"value": "warn"
				}
			]
		},
		{
			"key": "plain",
			"type": "Radio",
			"label": "是否镂空"
		},
		{
			"key": "disabled",
			"type": "Radio",
			"label": "是否禁用"
		},
		{
			"key": "loading",
			"type": "Radio",
			"label": "是否带 loading 图标"
		},
		{
			"key": "hoverClass",
			"type": "Input",
			"label": "指定按下去的样式类"
		},
		{
			"key": "hoverStartTime",
			"type": "InputNumber",
			"label": "按住后多久出现点击态，单位毫秒"
		},
		{
			"key": "hoverStayTime",
			"type": "InputNumber",
			"label": "手指松开后点击态保留时间，单位毫秒"
		}
	]
}
